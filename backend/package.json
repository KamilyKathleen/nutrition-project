{
  "name": "nutrition-project-backend",
  "version": "1.0.0",
  "description": "Backend para Sistema de Planejamento e Avaliação de Dietas",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --runInBand --passWithNoTests",
    "test:watch": "jest --watch --runInBand",
    "test:unit": "jest tests/services --runInBand",
    "test:integration": "jest tests/integration --runInBand",
    "test:usability": "jest tests/usability --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --runInBand --coverage --ci",
    "test:safe": "node scripts/test-runner.js",
    "test:heavy": "ENABLE_HEAVY_TESTS=true node scripts/test-runner.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "mongoose": "^8.0.3",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^5.9.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "nutrition",
    "diet",
    "health",
    "node",
    "express",
    "typescript",
    "firebase"
  ],
  "author": "Nutrition Project Team",
  "license": "MIT"
}
